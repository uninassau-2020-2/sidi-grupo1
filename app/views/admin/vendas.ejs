<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Vendas</title>
		</head>
	<body>

		<h1>VENDAS</h1>

		<table>
			<tr>
				<td>ID</td>
				<td>Produto</td>
				<td>Funcionario</td>
				<td>Data</td>
			</tr>

			<% for(var i=0; i < vendas.length; i++) { %>
				<tr>
					<td><%= vendas[i].id %></td>
					<td><%= vendas[i].id_produto %></td>
					<td><%= vendas[i].id_funcionario %></td>
					<td><%= vendas[i].date %></td>
				</tr>
			<% } %>
		</table>

	</body>
</html>